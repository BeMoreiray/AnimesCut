package com.projects.animescut.services;

import java.util.Optional;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.projects.animescut.repositories.GenericRepository;

@Service
public class GenericService<T, ID> {
	
	@Autowired
	private GenericRepository<T, ID> repository;
	
	protected boolean hasDuplication( Object entity) {
		Optional<T> existingEntity = repository.findByDuplicateFild(entity);
		
	}
}
